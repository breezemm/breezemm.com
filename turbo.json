{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["prebuild", "^build"],
        "outputs": [".next/**", "!.next/cache/**"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "start": {
        "dependsOn": ["^build"],
        "cache": false,
        "persistent": true
      },
      "dev:test": {
        "cache": false
      }
    }
}